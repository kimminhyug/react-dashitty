{
  "name": "dashboardity",
  "version": "1.2.1",
  "description": "Dashboard library: grid layout, panels, widgets, app shell",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rimraf dist",
    "example": "vite --config examples/playground/vite.config.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@dashboardity/layout-core": "^1.0.0",
    "@dashboardity/layout-store": "^1.0.0",
    "react-griditty": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "dashboard",
    "react",
    "widgets",
    "grid",
    "layout"
  ]
}
